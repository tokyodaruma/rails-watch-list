<%= simple_form_for(list) do |f| %>
  <div class="container">
    <div class="row g-3">
      <div class="col-6">
        <%= f.input :name, placeholder: 'Best Movie List Eva'%>
      </div>
      <div class="col-6">
        <%= f.input :photo, as: :file %>
      </div>
      <div class="col align-self-center">
        <%= f.submit %>
      </div>
    </div>
  </div>
<% end %>
